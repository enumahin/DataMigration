<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane prefHeight="597.0" prefWidth="806.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.ccfng.apibasedmigration.Controller">
   <children>
      <VBox prefHeight="45.0" prefWidth="806.0" style="-fx-background-color: #336699;" />
      <VBox layoutY="45.0" prefHeight="552.0" prefWidth="212.0">
         <children>
            <ComboBox fx:id="stateCombo" onAction="#getLgas" prefHeight="27.0" prefWidth="213.0" promptText="Select State">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </ComboBox>
            <ComboBox fx:id="lgaCombo" layoutX="10.0" layoutY="10.0" onAction="#getFacilities" prefHeight="27.0" prefWidth="213.0" promptText="Select LGA">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </ComboBox>
            <ComboBox fx:id="facilityCombo" layoutX="10.0" layoutY="37.0" prefHeight="27.0" prefWidth="213.0" promptText="Select Facility Name">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </ComboBox>
            <TextField fx:id="datimCode" promptText="Datim Code">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#buildData" prefHeight="27.0" prefWidth="212.0" text="Build Data">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <HBox layoutX="212.0" layoutY="45.0" prefHeight="552.0" prefWidth="594.0">
         <children>
            <TreeView fx:id="patientTree" prefHeight="552.0" prefWidth="599.0" />
         </children>
      </HBox>
   </children>
</AnchorPane>
