<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="589.0" prefWidth="1266.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.ccfng.viralload.Controller">

	<children>
      <HBox prefHeight="435.0" prefWidth="1266.0">
         <children>
      		<VBox prefHeight="435.0" prefWidth="1078.0">
      			<children>
      				<TableView fx:id="vlTableView" prefHeight="436.0" prefWidth="1077.0">
      					<columns>
							<TableColumn prefWidth="97.0" text="Patient ID">
								<cellValueFactory>
									<PropertyValueFactory property="PatientID" />
								</cellValueFactory>
							</TableColumn>
							<TableColumn prefWidth="133.0" text="PepFar ID">
								<cellValueFactory>
									<PropertyValueFactory property="PepfarID" />
								</cellValueFactory>
							</TableColumn>
							<TableColumn prefWidth="173.0" text="Patient Name">
								<cellValueFactory>
									<PropertyValueFactory property="PatientName" />
								</cellValueFactory>
							</TableColumn>
							<TableColumn prefWidth="166.0" text="Phone Number">
								<cellValueFactory>
									<PropertyValueFactory property="patientPhoneNumber" />
								</cellValueFactory>
							</TableColumn>
      						<TableColumn prefWidth="166.0" text="ART Start Date">
								<cellValueFactory>
									<PropertyValueFactory property="artStartDate" />
								</cellValueFactory>
							</TableColumn>
      						<TableColumn prefWidth="166.0" text="Last VL Date">
								<cellValueFactory>
									<PropertyValueFactory property="lastVLDate" />
								</cellValueFactory>
							</TableColumn>
      						<TableColumn prefWidth="166.0" text="VL Count">
								<cellValueFactory>
									<PropertyValueFactory property="vlCount" />
								</cellValueFactory>
							</TableColumn>
      						<TableColumn prefWidth="166.0" text="VL Due Date" />
      						<TableColumn prefWidth="166.0" text="Reason" />
      					</columns>
      				</TableView>
      			</children>
      		</VBox>
            <VBox prefHeight="435.0" prefWidth="190.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="190.0" style="-fx-background-color: #336699;" text="Actions" textFill="#f8f7f7">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </Label>
                  <Button mnemonicParsing="false" onAction="#getVlEligibility" prefHeight="31.0" prefWidth="189.0" text="Load" />
                  <Button mnemonicParsing="false" onAction="#vlToExcel" prefHeight="31.0" prefWidth="189.0" text="Export" />
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0" />
      <TextArea fx:id="appConsole" layoutX="-1.0" layoutY="435.0" prefHeight="196.0" prefWidth="1266.0" />
	</children>

</AnchorPane>
