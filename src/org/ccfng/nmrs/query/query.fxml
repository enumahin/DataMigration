<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="514.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.ccfng.nmrs.query.Controller">
   <children>
      <HBox layoutY="1.0" prefHeight="455.0" prefWidth="610.0">
         <children>
            <TextArea fx:id="query" prefHeight="403.0" prefWidth="610.0" promptText="Type your query here......" />
         </children>
      </HBox>
      <HBox layoutY="455.0" prefHeight="62.0" prefWidth="610.0">
         <children>
            <Button mnemonicParsing="false" onAction="#executeQuery" prefHeight="61.0" prefWidth="614.0" style="-fx-background-color: darkgreen;" text="Execute" textFill="#ebe8e8">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
