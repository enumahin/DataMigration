<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="524.0" prefWidth="806.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.ccfng.nmrs.editform.Controller">
   <children>
      <HBox layoutY="-2.0" prefHeight="108.0" prefWidth="806.0">
         <children>
            <VBox maxHeight="-Infinity" prefHeight="65.0" prefWidth="841.0">
               <children>
                  <Label prefHeight="65.0" prefWidth="422.0" text="Form Editor">
                     <font>
                        <Font name="System Bold" size="26.0" />
                     </font>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </Label>
               </children>
            </VBox>
            <VBox maxHeight="-Infinity" prefHeight="58.0" prefWidth="808.0">
               <children>
                  <ComboBox fx:id="cboForm" prefHeight="68.0" prefWidth="389.0" promptText="Select Form" />
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox layoutY="106.0" prefHeight="169.0" prefWidth="806.0">
         <children>
            <TextArea fx:id="txtFind" prefHeight="169.0" prefWidth="809.0" promptText="FIND" />
         </children>
      </HBox>
      <HBox layoutY="273.0" prefHeight="158.0" prefWidth="811.0">
         <children>
            <TextArea fx:id="txtReplace" prefHeight="158.0" prefWidth="819.0" promptText="REPLACE" />
         </children>
      </HBox>
      <HBox layoutX="-1.0" layoutY="431.0" prefHeight="93.0" prefWidth="811.0">
         <children>
            <HBox prefHeight="93.0" prefWidth="439.0">
               <children>
                  <Button fx:id="btnReplace" mnemonicParsing="false" onAction="#replace" prefHeight="97.0" prefWidth="437.0" style="-fx-background-color: darkgreen;" text="Replace" textFill="#eee9e9">
                     <font>
                        <Font name="System Bold Italic" size="20.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <HBox prefHeight="93.0" prefWidth="376.0">
               <children>
                  <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#close" prefHeight="95.0" prefWidth="376.0" style="-fx-background-color: brown;" text="Cancel">
                     <font>
                        <Font name="System Bold Italic" size="20.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
